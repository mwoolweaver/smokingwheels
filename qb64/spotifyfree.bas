REM Windows 10 traffic saver for wifi hot spots
REM
REM
REM
REM

'TIMER ON
'ON TIMER(1) GOSUB health
DO
    a$ = INKEY$
    IF a$ = CHR$(27) THEN STOP
    LOCATE 10, 15
    PRINT "1 = Block adds and save traffic"
    LOCATE 11, 15

    PRINT "2 = Allow adds "
    LOCATE 12, 15

    PRINT "ESC to Exit"
    IF a$ = "1" THEN GOSUB blockupdates
    IF a$ = "2" THEN GOSUB unblockupdate
    _LIMIT 1
LOOP

unblockupdate:
OPEN "c:\Windows\System32\drivers\etc\hosts" FOR OUTPUT AS #2
PRINT #2, "127.0.0.1       localhost"
PRINT #2, "::1             localhost"
PRINT " Unlocked updates at  "; TIME$
CLOSE #2: CLOSE #3

RETURN


